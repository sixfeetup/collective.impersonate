<?xml version="1.0" encoding="utf-8"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <action-provider name="portal_actions"/>
 <object name="user" meta_type="CMF Action Category">
  <property name="title">User actions</property>
  <object name="impersonate" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Impersonate</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/@@impersonate</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:not context.REQUEST.get('impersonate')</property>
   <property name="permissions">
    <element value="Manage Site"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="logout-impersonate" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Log out impersonated user</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/@@logout-impersonate</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:context.REQUEST.get('impersonate')</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
